<h1>Sign Up</h1>
<%= form_for @user do |f|%>
  <%= f.label :username%><br>
  <%= f.text_field :username%><br>
  <%= f.label :full_name%><br>
  <%= f.text_field :full_name%><br>
  <%= f.label :password%><br>
  <%= f.password_field :password%><br>
  <%= f.label :profile_photo%><br>
  <%= f.file_field :profile_photo%><br>
  <br>
  <%= f.submit %>
  
<%end%>
